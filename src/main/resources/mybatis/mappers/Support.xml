<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="itc.hoseo.oversea.support.SupportRepository">
    <select id="getQnaArticles" resultType="Support" parameterType="Support">
		select * from qnaboard 
		<where>
          <if test="@org.thymeleaf.util.StringUtils@isEmptyOrWhitespace(keyField) == subject">
            and subject like '%' ||  #{keyWord} || '%'
         </if>
          <if test="@org.thymeleaf.util.StringUtils@isEmptyOrWhitespace(keyField) == writer">
            and writer like '%' ||  #{keyWord} || '%'
         </if>
      </where>
      order by ref desc, reStep asc limit #{start}, #{end}
		 				
	</select>
	
</mapper>